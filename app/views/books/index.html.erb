

<%= link_to user_path(current_user.id),class:"btn btn-outline-secondary btn-block" do %>
<% end %>



<h2>New book</h2>

<% @books.each do |book| %>
<tr>
  <td><%= link_to book_path(book.id) do %></td>
      <%= image_tag book.user.get_profile_image(200, 200) %>
      <% end %>
  <td><%= book.body %></td>
  </tr>



<% end %>